@model AddPostFormModel

@{
    ViewData["Title"] = "Add Post";
}

<div class="row sec14">
    <div class="col-sm-12 addpostform">
        <form method="post">
            <h1>Add your post</h1>
            <partial name="_PostFormPartial" model="@Model" />

            <input class="btnaddpost" type="submit" value="Add Post" />
        </form>
    </div>
</div>

